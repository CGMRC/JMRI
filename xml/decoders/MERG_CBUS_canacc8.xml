<decoder-config xmlns:xi="http://www.w3.org/2001/XInclude" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://jmri.org/xml/schema/decoder-4-15-2.xsd">
    <copyright xmlns="http://docbook.org/ns/docbook">
        <year>2023</year>
        <holder>JMRI</holder>
    </copyright>
    <authorgroup xmlns="http://docbook.org/ns/docbook">
        <author>
            <personname>
                <firstname>Andrew</firstname>
                <surname>Crosland</surname>
            </personname>
        </author>
    </authorgroup>
    <revhistory xmlns="http://docbook.org/ns/docbook">
        <revision>
            <revnumber>1</revnumber>
            <date>2002?</date>
            <authorinitials>BJ</authorinitials>
            <revremark>New File</revremark>
        </revision>
    </revhistory>
    <decoder>
        <family name="CBUS" mfg="MERG" comment="CANACC8 is a CBUS 8 outpout module.">
            <model model="CANACC8" lowVersionID="10" highVersionID="10"/>
        </family>
        <programming direct="yes" ops="yes" paged="yes" register="yes">
        </programming>
        <variables>
            <variable item="rep1" CV="10001" mask="VXXXXXXX" default="1">
                <xi:include href="http://jmri.org/xml/decoders/parts/enum-NoYes.xml"/>
            </variable>
            <variable item="rep2" CV="10002" mask="VXXXXXXX" default="1">
                <xi:include href="http://jmri.org/xml/decoders/parts/enum-NoYes.xml"/>
            </variable>
            <variable item="rep3" CV="10003" mask="VXXXXXXX" default="1">
                <xi:include href="http://jmri.org/xml/decoders/parts/enum-NoYes.xml"/>
            </variable>
            <variable item="rep4" CV="10004" mask="VXXXXXXX" default="1">
                <xi:include href="http://jmri.org/xml/decoders/parts/enum-NoYes.xml"/>
            </variable>
            <variable item="rep5" CV="10005" mask="VXXXXXXX" default="1">
                <xi:include href="http://jmri.org/xml/decoders/parts/enum-NoYes.xml"/>
            </variable>
            <variable item="rep6" CV="10006" mask="VXXXXXXX" default="1">
                <xi:include href="http://jmri.org/xml/decoders/parts/enum-NoYes.xml"/>
            </variable>
            <variable item="rep7" CV="10007" mask="VXXXXXXX" default="1">
                <xi:include href="http://jmri.org/xml/decoders/parts/enum-NoYes.xml"/>
            </variable>
            <variable item="rep8" CV="10008" mask="VXXXXXXX" default="1">
                <xi:include href="http://jmri.org/xml/decoders/parts/enum-NoYes.xml"/>
            </variable>
        
            <variable item="width1" CV="10001" mask="XVVVVVVV" default="0">
                <decVal min="0" max="127"/>
            </variable>
            <variable item="width2" CV="10002" mask="XVVVVVVV" default="0">
                <decVal min="0" max="127"/>
            </variable>
            <variable item="width3" CV="10003" mask="XVVVVVVV" default="0">
                <decVal min="0" max="127"/>
            </variable>
            <variable item="width4" CV="10004" mask="XVVVVVVV" default="0">
                <decVal min="0" max="127"/>
            </variable>
            <variable item="width5" CV="10005" mask="XVVVVVVV" default="0">
                <decVal min="0" max="127"/>
            </variable>
            <variable item="width6" CV="10006" mask="XVVVVVVV" default="0">
                <decVal min="0" max="127"/>
            </variable>
            <variable item="width7" CV="10007" mask="XVVVVVVV" default="0">
                <decVal min="0" max="127"/>
            </variable>
            <variable item="width8" CV="10008" mask="XVVVVVVV" default="0">
                <decVal min="0" max="127"/>
            </variable>
        
            <variable default="1" item="startupActtion1" mask="XXXXXXXV XXXXXXXV" CV="11,10">
                <xi:include href="http://jmri.org/xml/decoders/cbus/EnumAccStartup.xml"/>
            </variable>
            <variable default="1" item="startupActtion2" mask="XXXXXXVX XXXXXXVX" CV="11,10">
                <xi:include href="http://jmri.org/xml/decoders/cbus/EnumAccStartup.xml"/>
            </variable>
            <variable default="1" item="startupActtion3" mask="XXXXXVXX XXXXXVXX" CV="11,10">
                <xi:include href="http://jmri.org/xml/decoders/cbus/EnumAccStartup.xml"/>
            </variable>
            <variable default="1" item="startupActtion4" mask="XXXXVXXX XXXXVXXX" CV="11,10">
                <xi:include href="http://jmri.org/xml/decoders/cbus/EnumAccStartup.xml"/>
            </variable>
            <variable default="1" item="startupActtion5" mask="XXXVXXXX XXXVXXXX" CV="11,10">
                <xi:include href="http://jmri.org/xml/decoders/cbus/EnumAccStartup.xml"/>
            </variable>
            <variable default="1" item="startupActtion6" mask="XXVXXXXX XXVXXXXX" CV="11,10">
                <xi:include href="http://jmri.org/xml/decoders/cbus/EnumAccStartup.xml"/>
            </variable>
            <variable default="1" item="startupActtion7" mask="XVXXXXXX XVXXXXXX" CV="11,10">
                <xi:include href="http://jmri.org/xml/decoders/cbus/EnumAccStartup.xml"/>
            </variable>
            <variable default="1" item="startupActtion8" mask="VXXXXXXX VXXXXXXX" CV="11,10">
                <xi:include href="http://jmri.org/xml/decoders/cbus/EnumAccStartup.xml"/>
            </variable>
        
            <variable default ="0" item="delay" CV="9">
                <decVal min="0" max="255"/>
            </variable>
        </variables>
    </decoder>
    <pane>
        <name>Setup</name>
        <column>
            <grid ipadx="10" ipady="2">  
                <griditem gridx="0" gridy="0" weightx="1" anchor="LINE_END">
                    <label> 
                        <text> Servo 1</text> 
                    </label>
                </griditem>
                <griditem gridx="0" gridy="1" weightx="1" anchor="LINE_END">
                    <display item="rep1" format="checkbox" label="Continuous" tooltip="Select continuous for output 1" />
                </griditem>
                <griditem gridx="1" gridy="1" weightx="1" anchor="LINE_END">
                    <display item="width1" label="Pulse Width (x 20 ms)" layout="above" tooltip="Pulse width in 20 ms increments for output 1" />
                </griditem>
                <griditem gridx="2" gridy="1" weightx="1" anchor="LINE_END">
                    <display item="startupActtion1" label="Startup Action" layout="above" tooltip="Select startup action for servo 1" />
                </griditem>
                
                <griditem gridx="0" gridy="2" weightx="1" anchor="LINE_END">
                    <display item="rep2" format="checkbox" label="Continuous" tooltip="Select continuous for output 2" />
                </griditem>
                <griditem gridx="1" gridy="2" weightx="1" anchor="LINE_END">
                    <display item="width2" label="Pulse Width (x 20 ms)" layout="above" tooltip="Pulse width in 20 ms increments for output 2" />
                </griditem>
                <griditem gridx="2" gridy="2" weightx="1" anchor="LINE_END">
                    <display item="startupActtion2" label="Startup Action" layout="above" tooltip="Select startup action for output 2" />
                </griditem>

                <griditem gridx="0" gridy="3" weightx="1" anchor="LINE_END">
                    <display item="rep3" format="checkbox" label="Continuous" tooltip="Select continuous for output 3" />
                </griditem>
                <griditem gridx="1" gridy="3" weightx="1" anchor="LINE_END">
                    <display item="width3" label="Pulse Width (x 20 ms)" layout="above" tooltip="Pulse width in 20 ms increments for output 3" />
                </griditem>
                <griditem gridx="2" gridy="3" weightx="1" anchor="LINE_END">
                    <display item="startupActtion3" label="Startup Action" layout="above" tooltip="Select startup action for output 3" />
                </griditem>
                
                <griditem gridx="0" gridy="4" weightx="1" anchor="LINE_END">
                    <display item="rep4" format="checkbox" label="Continuous" tooltip="Select continuous for output 4" />
                </griditem>
                <griditem gridx="1" gridy="4" weightx="1" anchor="LINE_END">
                    <display item="width4" label="Pulse Width (x 20 ms)" layout="above" tooltip="Pulse width in 20 ms increments for output 4" />
                </griditem>
                <griditem gridx="2" gridy="4" weightx="1" anchor="LINE_END">
                    <display item="startupActtion4" label="Startup Action" layout="above" tooltip="Select startup action for output 4" />
                </griditem>

                <griditem gridx="0" gridy="5" weightx="1" anchor="LINE_END">
                    <display item="rep5" format="checkbox" label="Continuous" tooltip="Select continuous for output 5" />
                </griditem>
                <griditem gridx="1" gridy="5" weightx="1" anchor="LINE_END">
                    <display item="width5" label="Pulse Width (x 20 ms)" layout="above" tooltip="Pulse width in 20 ms increments for output 5" />
                </griditem>
                <griditem gridx="2" gridy="5" weightx="1" anchor="LINE_END">
                    <display item="startupActtion5" label="Startup Action" layout="above" tooltip="Select startup action for output 5" />
                </griditem>
                
                <griditem gridx="0" gridy="6" weightx="1" anchor="LINE_END">
                    <display item="rep6" format="checkbox" label="Continuous" tooltip="Select continuous for output 6" />
                </griditem>
                <griditem gridx="1" gridy="6" weightx="1" anchor="LINE_END">
                    <display item="width6" label="Pulse Width (x 20 ms)" layout="above" tooltip="Pulse width in 20 ms increments for output 6" />
                </griditem>
                <griditem gridx="2" gridy="6" weightx="1" anchor="LINE_END">
                    <display item="startupActtion6" label="Startup Action" layout="above" tooltip="Select startup action for output 6" />
                </griditem>

                <griditem gridx="0" gridy="7" weightx="1" anchor="LINE_END">
                    <display item="rep7" format="checkbox" label="Continuous" tooltip="Select continuous for output 7" />
                </griditem>
                <griditem gridx="1" gridy="7" weightx="1" anchor="LINE_END">
                    <display item="width7" label="Pulse Width (x 20 ms)" layout="above" tooltip="Pulse width in 20 ms increments for output 7" />
                </griditem>
                <griditem gridx="2" gridy="7" weightx="1" anchor="LINE_END">
                    <display item="startupActtion7" label="Startup Action" layout="above" tooltip="Select startup action for output 7" />
                </griditem>
                
                <griditem gridx="0" gridy="8" weightx="1" anchor="LINE_END">
                    <display item="rep8" format="checkbox" label="Continuous" tooltip="Select continuous for output 8" />
                </griditem>
                <griditem gridx="1" gridy="8" weightx="1" anchor="LINE_END">
                    <display item="width8" label="Pulse Width (x 20 ms)" layout="above" tooltip="Pulse width in 20 ms increments for output 8" />
                </griditem>
                <griditem gridx="2" gridy="8" weightx="1" anchor="LINE_END">
                    <display item="startupActtion8" label="Startup Action" layout="above" tooltip="Select startup action for output 8" />
                </griditem>

                <griditem gridx="1" gridy="9" weightx="3" anchor="LINE_END">
                    <display item="delay" label="Feedback Delay (x 0.5 ms)" layout="above" tooltip="Feedback delay in 0.5 ms increments" />
                </griditem>
            </grid>
        </column>
    </pane>
</decoder-config>
